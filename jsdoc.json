{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.(js|jsx)$",
    "excludePattern": "(node_modules|\\.test\\.js)"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/docdash",
    "encoding": "utf8"
  }
}